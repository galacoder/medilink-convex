{
  "name": "@medilink/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "dotenv --override -e ../../.env -e ../../.env.local -- next dev",
    "e2e": "pnpm with-env playwright test",
    "e2e:local": "PORT=3002 pnpm with-env-local playwright test",
    "e2e:ci": "playwright test --config=playwright.config.ci.ts",
    "e2e:ui": "PORT=3002 pnpm with-env-local playwright test --ui",
    "e2e:debug": "PORT=3002 pnpm with-env-local playwright test --debug",
    "vrt": "playwright test --config=playwright.config.vrt.ts",
    "vrt:update": "playwright test --config=playwright.config.vrt.ts --update-snapshots",
    "vrt:all": "pnpm vrt && PORT=3002 pnpm with-env-local playwright test --config=playwright.config.portal-vrt.ts",
    "vrt:all:update": "pnpm vrt:update && PORT=3002 pnpm with-env-local playwright test --config=playwright.config.portal-vrt.ts --update-snapshots",
    "vrt:portal": "PORT=3002 pnpm with-env-local playwright test --config=playwright.config.portal-vrt.ts",
    "vrt:portal:update": "PORT=3002 pnpm with-env-local playwright test --config=playwright.config.portal-vrt.ts --update-snapshots",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "start": "pnpm with-env next start",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env -e ../../.env.ci --",
    "with-env-local": "dotenv --override -e ../../.env -e ../../.env.local --"
  },
  "dependencies": {
    "@convex-dev/better-auth": "^0.10.10",
    "@copilotkit/react-core": "^1.51.4",
    "@copilotkit/react-ui": "^1.51.4",
    "@copilotkit/runtime": "^1.51.4",
    "@medilink/api": "workspace:*",
    "@medilink/auth": "workspace:*",
    "@medilink/db": "workspace:*",
    "@medilink/ui": "workspace:*",
    "@medilink/validators": "workspace:*",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-form": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "better-auth": "catalog:",
    "convex": "^1.31.7",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.574.0",
    "next": "^16.0.9",
    "openai": "^6.22.0",
    "qrcode": "^1.5.4",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-qr-code": "^2.0.18",
    "recharts": "^3.7.0",
    "superjson": "2.2.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@medilink/eslint-config": "workspace:*",
    "@medilink/prettier-config": "workspace:*",
    "@medilink/tailwind-config": "workspace:*",
    "@medilink/tsconfig": "workspace:*",
    "@next/bundle-analyzer": "^16.0.9",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "catalog:",
    "@types/qrcode": "^1.5.6",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "jiti": "^2.5.1",
    "jsdom": "^28.1.0",
    "prettier": "catalog:",
    "tailwindcss": "catalog:",
    "tw-animate-css": "^1.4.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@medilink/prettier-config"
}
